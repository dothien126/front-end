{
  "name": "zimbra-webmail",
  "version": "1.0.0",
  "scripts": {
    "bootstrap": "npm install",
    "serve": "npm run dev",
    "dattqh": "cross-env vite --mode dattqh",
    "hobv": "cross-env vite --mode hobv",
    "khanhpd": "cross-env vite --mode khanhpd",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:test": "cross-env vite build --mode test",
    "build:no-cache": "npm clean:cache && npm run build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview --port 8012",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "reinstall": "rimraf package-lock.json && rimraf node_modules && npm run bootstrap"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckpack/vue-color": "^1.2.0",
