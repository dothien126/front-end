    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/list": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.1",
    "@googlemaps/js-api-loader": "^1.11.4",
    "@googlemaps/markerclustererplus": "^1.1.0",
    "@left4code/tw-starter": "^3.1.35",
    "@popperjs/core": "^2.7.0",
    "@types/crypto-js": "^4.1.1",
    "@vuelidate/core": "^2.0.0-alpha.12",
    "@vuelidate/validators": "^2.0.0-alpha.11",
    "axios": "^0.24.0",
    "chart.js": "^3.7.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.4",
    "dropzone": "^5.7.6",
    "file-saver": "^2.0.5",
    "install": "^0.13.0",
    "js-beautify": "^1.13.5",
    "litepicker": "^2.0.11",
    "lucide": "^0.84.0",
    "lucide-vue-next": "^0.84.0",
    "mitt": "^3.0.0",
    "mosha-vue-toastify": "^1.0.23",
    "npm": "^9.2.0",
    "pinia": "^2.0.9",
    "read-excel-file": "^5.5.3",
    "simplebar": "^5.3.6",
    "tabulator-tables": "^4.9.3",
    "thenby": "^1.3.4",
    "tiny-slider": "^2.9.3",
    "tippy.js": "^6.2.7",
    "toastify-js": "^1.9.3",
    "tom-select": "^1.7.5",
    "typy": "^3.3.0",
    "uuid": "^8.3.2",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.45",
    "vue-inline-svg": "^3.1.0",
    "vue-input-tag": "^2.0.7",
    "vue-router": "4",
    "vue3-tags-input": "^1.0.2",
    "vue3-xlsx": "^1.1.2",
    "xlsx": "^0.16.9",
    "zoom-vanilla.js": "^2.0.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.1",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "10.4.7",
    "cross-env": "^7.0.3",
